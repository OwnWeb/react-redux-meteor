{
  "name": "react-redux-playground",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src",
    "validate": "npm-run-all --parallel validate-webpack:* lint",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "clean-dist": "rimraf dist",
    "copy-files": "cpy src/index.html dist",
    "clean-and-copy": "npm run clean-dist && npm run copy-files",
    "prebuild": "npm run clean-and-copy",
    "prebuild:prod": "npm run clean-and-copy",
    "build": "webpack --env.dev",
    "build:prod": "webpack --env.prod -p",
    "prestart": "npm run clean-and-copy",
    "start": "webpack-dev-server --env.dev --history-api-fallback --content-base dist"
  },
  "keywords": [],
  "author": "Philipp Sporrer <philipp.sporrer@commercetools.de>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.13.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.11.0",
    "cpy-cli": "1.0.1",
    "eslint": "2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.11.1",
    "eslint-plugin-jsx-a11y": "1.5.5",
    "eslint-plugin-react": "5.2.2",
    "json-loader": "0.5.4",
    "npm-run-all": "2.3.0",
    "rimraf": "2.5.4",
    "webpack": "2.1.0-beta.20",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-validator": "2.2.3"
  },
  "dependencies": {
    "asteroid": "2.0.2",
    "lodash": "4.14.1",
    "meteor-random": "0.0.3",
    "node-uuid": "1.4.7",
    "normalizr": "2.2.1",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-redux": "4.4.5",
    "react-router": "3.0.0-alpha.2",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.1.0"
  }
}
